<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<ScrollPane fitToHeight="true" fitToWidth="true" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.xwintop.xJavaFxTool.controller.littleTools.CoordinateTransformToolController">
    <StackPane>
        <AnchorPane maxHeight="900" maxWidth="1200.0" minHeight="600" minWidth="900" prefHeight="453.0" prefWidth="748.0" StackPane.alignment="CENTER">
            <BorderPane prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
                <center>
                    <HBox spacing="20.0" BorderPane.alignment="CENTER">
                        <VBox spacing="10.0" HBox.hgrow="ALWAYS">
                            <Label text="源坐标" wrapText="true">
                                <font>
                                    <Font size="14.0" />
                                </font>
                            </Label>
                            <TextArea fx:id="sourceCoordinateTextArea" prefWidth="170.0" VBox.vgrow="ALWAYS">
                                <font>
                                    <Font size="14.0" />
                                </font>
                            </TextArea>
                        </VBox>
                        <VBox alignment="CENTER" prefWidth="230.0" spacing="20.0" HBox.hgrow="ALWAYS">
                            <Label text="tip: 源坐标采用换行标记一组坐标，一组坐标xy可以采用中英文逗号、制表符或空格分隔" wrapText="true">
                                <font>
                                    <Font size="14.0" />
                                </font>
                            </Label>
                            <VBox alignment="CENTER" style="-fx-border-width: 1px; -fx-border-color: #000; -fx-border-radius: 5px;">
                                <Label text="源坐标系">
                                    <font>
                                        <Font size="16.0" />
                                    </font>
                                    <VBox.margin>
                                        <Insets bottom="5.0" />
                                    </VBox.margin>
                                </Label>
                                <VBox>
                                    <GridPane hgap="15.0" vgap="5.0">
                                        <columnConstraints>
                                            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
                                            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
                                        </columnConstraints>
                                        <rowConstraints>
                                            <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                                            <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                                            <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                                        </rowConstraints>
                                        <HBox prefWidth="200.0" GridPane.columnIndex="0">
                                            <Label prefHeight="19.0" prefWidth="154.0" text="坐标类型：" HBox.hgrow="SOMETIMES">
                                                <font>
                                                    <Font size="14.0" />
                                                </font>
                                            </Label>
                                            <ChoiceBox fx:id="sourceCrsTypeChoiceBox" accessibleText="选择坐标类型" prefWidth="160.0" />
                                        </HBox>
                                        <HBox prefWidth="200.0" GridPane.columnIndex="1">
                                            <Label prefHeight="19.0" prefWidth="154.0" text="坐标参数：" HBox.hgrow="SOMETIMES">
                                                <font>
                                                    <Font size="14.0" />
                                                </font>
                                            </Label>
                                            <TextField fx:id="sourceCrsDesc" />
                                        </HBox>
                                        <HBox prefWidth="200.0" GridPane.columnIndex="0" GridPane.rowIndex="1">
                                            <Label prefHeight="19.0" prefWidth="154.0" text="X 偏移量：" HBox.hgrow="SOMETIMES">
                                                <font>
                                                    <Font size="14.0" />
                                                </font>
                                            </Label>
                                            <TextField fx:id="sourceOffsetX" />
                                        </HBox>
                                        <HBox prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
                                            <Label prefHeight="19.0" prefWidth="154.0" text="Y 偏移量：" HBox.hgrow="SOMETIMES">
                                                <font>
                                                    <Font size="14.0" />
                                                </font>
                                            </Label>
                                            <TextField fx:id="sourceOffsetY" />
                                        </HBox>
                                    </GridPane>
                                    <HBox prefWidth="200.0">
                                        <Label prefHeight="19.0" prefWidth="237.0" text="ESRI WKT：" HBox.hgrow="ALWAYS">
                                            <font>
                                                <Font size="14.0" />
                                            </font>
                                        </Label>
                                        <TextArea fx:id="sourceWkt" prefHeight="90.0" prefWidth="417.0" wrapText="true" HBox.hgrow="ALWAYS" />
                                    </HBox>
                                </VBox>
                                <padding>
                                    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                </padding>
                            </VBox>
                            <VBox alignment="CENTER" style="-fx-border-color: #000; -fx-border-radius: 5px; -fx-border-width: 1px;">
                                <Label text="目标坐标系">
                                    <font>
                                        <Font size="16.0" />
                                    </font>
                                    <VBox.margin>
                                        <Insets bottom="5.0" />
                                    </VBox.margin>
                                </Label>
                                <VBox>
                                    <GridPane hgap="15.0" vgap="5.0">
                                        <columnConstraints>
                                            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
                                            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
                                        </columnConstraints>
                                        <rowConstraints>
                                            <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                                            <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                                            <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                                        </rowConstraints>
                                        <HBox prefWidth="200.0" GridPane.columnIndex="0">
                                            <Label prefHeight="19.0" prefWidth="154.0" text="坐标类型：" HBox.hgrow="SOMETIMES">
                                                <font>
                                                    <Font size="14.0" />
                                                </font>
                                            </Label>
                                            <ChoiceBox fx:id="targetCrsTypeChoiceBox" accessibleText="选择坐标类型" prefWidth="160.0" />
                                        </HBox>
                                        <HBox prefWidth="200.0" GridPane.columnIndex="1">
                                            <Label prefHeight="19.0" prefWidth="154.0" text="坐标参数：" HBox.hgrow="SOMETIMES">
                                                <font>
                                                    <Font size="14.0" />
                                                </font>
                                            </Label>
                                            <TextField fx:id="targetCrsDesc" />
                                        </HBox>
                                        <HBox prefWidth="200.0" GridPane.columnIndex="0" GridPane.rowIndex="1">
                                            <Label prefHeight="19.0" prefWidth="154.0" text="X 偏移量：" HBox.hgrow="SOMETIMES">
                                                <font>
                                                    <Font size="14.0" />
                                                </font>
                                            </Label>
                                            <TextField fx:id="targetOffsetX" />
                                        </HBox>
                                        <HBox prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
                                            <Label prefHeight="19.0" prefWidth="154.0" text="Y 偏移量：" HBox.hgrow="SOMETIMES">
                                                <font>
                                                    <Font size="14.0" />
                                                </font>
                                            </Label>
                                            <TextField fx:id="targetOffsetY" />
                                        </HBox>
                                    </GridPane>
                                    <HBox prefWidth="200.0">
                                        <Label prefHeight="19.0" prefWidth="237.0" text="ESRI WKT：" HBox.hgrow="ALWAYS">
                                            <font>
                                                <Font size="14.0" />
                                            </font>
                                        </Label>
                                        <TextArea fx:id="targetWkt" prefHeight="90.0" prefWidth="421.0" wrapText="true" HBox.hgrow="ALWAYS" />
                                    </HBox>
                                </VBox>
                                <VBox.margin>
                                    <Insets />
                                </VBox.margin>
                                <padding>
                                    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                </padding>
                            </VBox>
                            <Button fx:id="transformButton" mnemonicParsing="false" onMouseClicked="#transformAction" text="开始转换">
                                <font>
                                    <Font size="14.0" />
                                </font>
                                <VBox.margin>
                                    <Insets />
                                </VBox.margin>
                            </Button>
                        </VBox>
                        <VBox spacing="10.0" HBox.hgrow="ALWAYS">
                            <Label text="目标坐标" wrapText="true">
                                <font>
                                    <Font size="14.0" />
                                </font>
                            </Label>
                            <TextArea fx:id="targetCoordinateTextArea" prefWidth="170.0" VBox.vgrow="ALWAYS">
                                <font>
                                    <Font size="14.0" />
                                </font>
                            </TextArea>
                        </VBox>
                    </HBox>
                </center>
            </BorderPane>
        </AnchorPane>
    </StackPane>
</ScrollPane>
